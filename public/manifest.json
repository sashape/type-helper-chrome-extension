{
  "manifest_version": 3,
  "name": "Type AI Helper",
  "version": "0.1",
  "description": "This extension helps you write anything for anyone",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/icons/icon16.png",
    "32": "assets/icons/icon32.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "permissions": [
    "activeTab"
  ]
}